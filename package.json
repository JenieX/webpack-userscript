{
  "name": "webpack-userscript",
  "version": "3.0.0-alpha.1",
  "description": "A Webpack plugin for userscript projects.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/momocow/webpack-userscript.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.prod.json",
    "commit": "gitmoji -c",
    "coverage": "",
    "lint": "eslint lib test",
    "prune": "shx rm -r dist",
    "release": "semantic-release",
    "test": "tape -r ts-node/register -r source-map-support/register",
    "test:all": "nyc tape -r ts-node/register -r source-map-support/register test/**/*.test.ts | tap-nyc"
  },
  "keywords": [
    "webpack",
    "userscript",
    "tampermonkey",
    "greasemonkey"
  ],
  "author": "MomoCow",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/momocow/webpack-userscript/issues"
  },
  "homepage": "https://github.com/momocow/webpack-userscript#readme",
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "@types/sinon": "^10.0.2",
    "@types/ssri": "^7.1.0",
    "@types/tape": "^4.13.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-typescript-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^4.3.0",
    "eslint-plugin-standard": "^5.0.0",
    "gitmoji-cli": "^4.1.0",
    "lint-staged": "^11.0.0",
    "memfs": "^3.2.2",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "semantic-release": "^17.4.4",
    "semantic-release-gitmoji": "^1.3.4",
    "shx": "^0.3.3",
    "sinon": "^11.1.1",
    "slash": "^3.0.0",
    "source-map-support": "^0.5.19",
    "tap-nyc": "^1.0.3",
    "tape": "^5.2.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2"
  },
  "peerDependencies": {
    "webpack": "^5.0.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "pkg-up": "^3.1.0",
    "schema-utils": "^3.0.0",
    "ssri": "^8.0.1"
  }
}
